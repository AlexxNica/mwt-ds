{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01-preview/deploymentTemplate.json",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "onlineTrainerCspkgToken": { "type": "string" },
    "configLink": { "type": "string" },
    "name": { "type": "string" },
    "location": { "type": "string" }
  },
  "resources": [
    {
      "apiVersion": "2014-06-01",
      "type": "Microsoft.ClassicCompute/domainNames",
      "name": "[parameters('name')]",
      "location": "[parameters('location')]",
      "resources": [
        {
          "apiVersion": "2014-06-01",
          "name": "production",
          "type": "deploymentSlots",
          "dependsOn": [
            "[resourceId('Microsoft.ClassicCompute/domainNames', parameters('name'))]"
          ],
          "properties": {
            "packageLink": {
              // "uri": "[parameters('onlineTrainerCspkgToken')]"
              "uri": "https://decisionservicestorage.blob.core.windows.net/mctest/azure_service.cspkg?sr=b&sv=2015-02-21&st=2016-07-29T15%3A48%3A28Z&se=2017-01-12T17%3A48%3A28Z&sp=rwd&sig=AmAgx8lxSxAf9IqJIuQeDkLhViUUUj6HxMHwkLIekKs%3D"
            },
            "deploymentLabel": "[parameters('name')]",
            "ConfigurationLink": {
              "uri": "[parameters('configLink')]"
            },
            "deploymentOptions": "StartDeployment"
          }
        }
      ]
    }
  ]
}