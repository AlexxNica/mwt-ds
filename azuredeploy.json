{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "variables": {
    "assetLocation": { "value": "https://raw.githubusercontent.com/multiworldtesting/ds-provisioning/master/" }
  },
  "resources": [
    {
      "apiVersion": "2015-01-01",
      "name": "Storage",
      "type": "Microsoft.Resources/deployments",
      "properties": {
        "mode": "incremental",
        "templateLink": {
          "uri": "[concat(variables('assetLocation'), 'templates/StorageTemplate.json')]",
          "contentVersion": "1.0.0.0"
        }
      }
    },
    {
      "apiVersion": "2015-01-01",
      "name": "OnlineTrainer",
      "type": "Microsoft.Resources/deployments",
      "dependsOn": [
        "Microsoft.Resources/deployments/Storage"
      ],
      "properties": {
        "mode": "incremental",
        "templateLink": {
          "uri": "[concat(variables('assetLocation'), 'templates/OnlineTrainerTemplate.json')]",
          "contentVersion": "1.0.0.0"
        },
        "parametersLink": {
          "uri": "[concat(variables('assetLocation'), 'templates/OnlineTrainerTemplate.parameters.json')]",
          "contentVersion": "1.0.0.0"
        }
      }
    }
  ]
}
